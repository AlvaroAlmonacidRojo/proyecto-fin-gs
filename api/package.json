  "scripts": {
    "clean": "rm -rf build",
    "compile": "tsc --build",
    "db:local": "cd ../../db && yarn db:local",
    "lint": "cd ../.. && yarn tslint --project ops/api",
    "start:dev:db": "yarn db:local up --build --detach",
    "start:dev:server": "tsc-watch --build --onSuccess 'yarn start'",
    "start:dev": "yarn start:dev:db && yarn start:dev:server",
    "start": "node --require dotenv/config build/server.js",
    "stop:dev:db": "yarn db:local down --volumes",
    "test:dev": "JEST_ENV=dev yarn run --silent test",
    "test": "jest"
  }
